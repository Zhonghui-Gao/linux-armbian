# 9 "arch/arm64/boot/dts/renesas/rz-smarc-cru-csi-ov5645.dtsi"
&{/} {
 ov5645_vdddo_1v8: 1p8v {
  compatible = "regulator-fixed";
  regulator-name = "camera_vdddo";
  regulator-min-microvolt = <1800000>;
  regulator-max-microvolt = <1800000>;
  regulator-always-on;
 };

 ov5645_vdda_2v8: 2p8v {
  compatible = "regulator-fixed";
  regulator-name = "camera_vdda";
  regulator-min-microvolt = <2800000>;
  regulator-max-microvolt = <2800000>;
  regulator-always-on;
 };

 ov5645_vddd_1v5: 1p5v {
  compatible = "regulator-fixed";
  regulator-name = "camera_vddd";
  regulator-min-microvolt = <1500000>;
  regulator-max-microvolt = <1500000>;
  regulator-always-on;
 };

 ov5645_fixed_clk: osc25250-clk {
  compatible = "fixed-clock";
  #clock-cells = <0>;
  clock-frequency = <24000000>;
 };
};

&cru {
 status = "okay";
};

&csi2 {
 status = "okay";

 ports {
  port@0 {
   csi2_in: endpoint {
    clock-lanes = <0>;
    data-lanes = <1 2>;
    remote-endpoint = <&ov5645_ep>;
   };
  };
 };
};

&i2c0 {
 #address-cells = <1>;
 #size-cells = <0>;

 ov5645: camera@3c {
  compatible = "ovti,ov5645";
  reg = <0x3c>;
  clocks = <&ov5645_fixed_clk>;
  clock-frequency = <24000000>;
  vdddo-supply = <&ov5645_vdddo_1v8>;
  vdda-supply = <&ov5645_vdda_2v8>;
  vddd-supply = <&ov5645_vddd_1v5>;

  port {
   ov5645_ep: endpoint {
    clock-lanes = <0>;
    data-lanes = <1 2>;
    remote-endpoint = <&csi2_in>;
   };
  };
 };
};
# 17 "arch/arm64/boot/dts/renesas/r9a07g044l2-smarc-cru-csi-ov5645.dtso" 2

&ov5645 {
 enable-gpios = <&pinctrl ((2) * 8 + (0)) 0>;
 reset-gpios = <&pinctrl ((40) * 8 + (2)) 1>;
};
