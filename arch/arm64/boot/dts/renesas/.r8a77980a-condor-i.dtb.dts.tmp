   #address-cells = <1>;
   #size-cells = <0>;

   i2c@0 {
    #address-cells = <1>;
    #size-cells = <0>;
    reg = <0>;

    status = "disabled";
   };

   i2c@1 {
    #address-cells = <1>;
    #size-cells = <0>;
    reg = <1>;

    status = "disabled";
   };

   i2c@2 {
    #address-cells = <1>;
    #size-cells = <0>;
    reg = <2>;

    status = "disabled";
   };

   i2c@3 {
    #address-cells = <1>;
    #size-cells = <0>;
    reg = <3>;

    status = "disabled";
   };
  };
 };

 gmsl1: gmsl-deserializer@4a {
