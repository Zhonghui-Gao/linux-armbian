   port@1 {
    reg = <1>;
   };

   port@2 {
    reg = <2>;
   };

   port@3 {
    reg = <3>;
   };

   port@4 {
    reg = <4>;
    max9286_out1: endpoint {
     clock-lanes = <0>;
     data-lanes = <1 2 3 4>;
     remote-endpoint = <&csi41_in>;
    };
   };
  };

  i2c-mux {
   #address-cells = <1>;
   #size-cells = <0>;

   i2c@0 {
    #address-cells = <1>;
    #size-cells = <0>;
    reg = <0>;

    status = "disabled";
   };

   i2c@1 {
    #address-cells = <1>;
    #size-cells = <0>;
    reg = <1>;

    status = "disabled";
   };

   i2c@2 {
    #address-cells = <1>;
    #size-cells = <0>;
    reg = <2>;

    status = "disabled";
   };

   i2c@3 {
    #address-cells = <1>;
    #size-cells = <0>;
    reg = <3>;

    status = "disabled";
   };
  };
 };
};

