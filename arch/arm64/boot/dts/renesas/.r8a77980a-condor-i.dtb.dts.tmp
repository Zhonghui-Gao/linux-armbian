  compatible = "maxim,max9286";
  reg = <0x48>;

  maxim,gpio-poc = <0 1>;
  enable-gpios = <&io_expander0 0 0>;

  ports {
   #address-cells = <1>;
   #size-cells = <0>;

   port@0 {
    reg = <0>;
   };

   port@1 {
    reg = <1>;
   };

   port@2 {
    reg = <2>;
   };

   port@3 {
    reg = <3>;
   };

   port@4 {
    reg = <4>;
    max9286_out0: endpoint {
     clock-lanes = <0>;
     data-lanes = <1 2 3 4>;
     remote-endpoint = <&csi40_in>;
