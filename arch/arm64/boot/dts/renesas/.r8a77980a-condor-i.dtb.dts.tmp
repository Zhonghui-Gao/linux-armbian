 vmmc-supply = <&d3_3v>;
 vqmmc-supply = <&vddq_vin01>;
 mmc-hs200-1_8v;
 bus-width = <8>;
 no-sd;
 no-sdio;
 non-removable;
 status = "okay";
};

&pciec {
 status = "okay";
};

&pcie_bus_clk {
 clock-frequency = <100000000>;
