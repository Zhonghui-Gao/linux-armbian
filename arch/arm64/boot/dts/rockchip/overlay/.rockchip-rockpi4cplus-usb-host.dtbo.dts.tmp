# 0 "arch/arm64/boot/dts/rockchip/overlay/rockchip-rockpi4cplus-usb-host.dts"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlay/rockchip-rockpi4cplus-usb-host.dts"


/dts-v1/;
/plugin/;

/ {
 compatible = "radxa,rockpi4c-plus", "radxa,rockpi4", "rockchip,rk3399";

 fragment@0 {
  target = <&usbdrd_dwc3_0>;
  __overlay__ {
   dr_mode = "host";
  };
 };
};
