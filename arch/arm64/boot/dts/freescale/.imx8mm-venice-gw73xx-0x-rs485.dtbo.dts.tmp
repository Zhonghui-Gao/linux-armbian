# 0 "arch/arm64/boot/dts/freescale/imx8mm-venice-gw73xx-0x-rs485.dtso"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/freescale/imx8mm-venice-gw73xx-0x-rs485.dtso"
# 14 "arch/arm64/boot/dts/freescale/imx8mm-venice-gw73xx-0x-rs485.dtso"
# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 15 "arch/arm64/boot/dts/freescale/imx8mm-venice-gw73xx-0x-rs485.dtso" 2

# 1 "arch/arm64/boot/dts/freescale/imx8mm-pinfunc.h" 1
# 17 "arch/arm64/boot/dts/freescale/imx8mm-venice-gw73xx-0x-rs485.dtso" 2

/dts-v1/;
/plugin/;

&{/} {
 compatible = "gw,imx8mm-gw73xx-0x";
};

&gpio4 {
 rs485_en {
  gpio-hog;
  gpios = <0 0>;
  output-high;
  line-name = "rs485_en";
 };

 rs485_hd {
  gpio-hog;
  gpios = <2 0>;
  output-high;
  line-name = "rs485_hd";
 };
};

&uart2 {
 pinctrl-names = "default";
 pinctrl-0 = <&pinctrl_uart2>;
 rts-gpios = <&gpio5 29 0>;
 linux,rs485-enabled-at-boot-time;
 status = "okay";
};

&uart4 {
 status = "disabled";
};

&iomuxc {
 pinctrl_uart2: uart2grp {
  fsl,pins = <
   0x23C 0x4A4 0x4FC 0x0 0x0 0x140
   0x240 0x4A8 0x000 0x0 0x0 0x140
   0x250 0x4B8 0x000 0x5 0x0 0x140
  >;
 };
};
