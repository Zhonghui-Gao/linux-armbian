# 0 "arch/arm64/boot/dts/freescale/imx8mm-venice-gw73xx-0x-imx219.dtso"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/freescale/imx8mm-venice-gw73xx-0x-imx219.dtso"





# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 7 "arch/arm64/boot/dts/freescale/imx8mm-venice-gw73xx-0x-imx219.dtso" 2

# 1 "arch/arm64/boot/dts/freescale/imx8mm-pinfunc.h" 1
# 9 "arch/arm64/boot/dts/freescale/imx8mm-venice-gw73xx-0x-imx219.dtso" 2

/dts-v1/;
/plugin/;

&{/} {
 compatible = "gw,imx8mm-gw73xx-0x", "fsl,imx8mm";

 reg_cam: regulator-cam {
  pinctrl-names = "default";
  pinctrl-0 = <&pinctrl_reg_cam>;
  compatible = "regulator-fixed";
  regulator-name = "reg_cam";
  gpio = <&gpio1 1 0>;
  enable-active-high;
  regulator-min-microvolt = <1800000>;
  regulator-max-microvolt = <1800000>;
 };

 cam24m: cam24m {
  compatible = "fixed-clock";
  #clock-cells = <0>;
  clock-frequency = <24000000>;
  clock-output-names = "cam24m";
 };
};

&csi {
 status = "okay";
};

&i2c3 {
 #address-cells = <1>;
 #size-cells = <0>;

 imx219: sensor@10 {
  compatible = "sony,imx219";
  reg = <0x10>;
  clocks = <&cam24m>;
  VDIG-supply = <&reg_cam>;

  port {

   imx219_to_mipi_csi2: endpoint {
    remote-endpoint = <&imx8mm_mipi_csi_in>;
    clock-lanes = <0>;
    data-lanes = <1 2>;
    link-frequencies = /bits/ 64 <456000000>;
   };
  };
 };
};

&mipi_csi {
 status = "okay";

 ports {
  #address-cells = <1>;
  #size-cells = <0>;

  port@0 {
   reg = <0>;

   imx8mm_mipi_csi_in: endpoint {
    remote-endpoint = <&imx219_to_mipi_csi2>;
    data-lanes = <1 2>;
   };
  };

  port@1 {
   reg = <1>;

   imx8mm_mipi_csi_out: endpoint {
    remote-endpoint = <&csi_in>;
   };
  };
 };
};

&iomuxc {
 pinctrl_reg_cam: regcamgrp {
  fsl,pins = <
   0x02C 0x294 0x000 0x0 0x0 0x41
  >;
 };
};
