    assigned-clocks = <&clk 125>,
        <&clk 126>,
        <&clk 92>,
        <&clk 78>,
        <&clk 79>;
    assigned-clock-parents = <&clk 54>,
        <&clk 2>,
        <&clk 40>,
        <&clk 65>,
        <&clk 56>;
    assigned-clock-rates = <266000000>,
             <24000000>,
             <594000000>,
             <500000000>,
             <200000000>;
    #power-domain-cells = <1>;
   };

   usbotg1: usb@32e40000 {
    compatible = "fsl,imx8mn-usb", "fsl,imx7d-usb", "fsl,imx27-usb";
    reg = <0x32e40000 0x200>;
