  };

  gpmi: nand-controller@33002000 {
   compatible = "fsl,imx8mn-gpmi-nand", "fsl,imx7d-gpmi-nand";
   #address-cells = <1>;
   #size-cells = <0>;
   reg = <0x33002000 0x2000>, <0x33004000 0x4000>;
   reg-names = "gpmi-nand", "bch";
   interrupts = <0 14 4>;
   interrupt-names = "bch";
   clocks = <&clk 156>,
     <&clk 192>;
   clock-names = "gpmi_io", "gpmi_bch_apb";
