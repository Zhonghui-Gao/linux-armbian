    clocks = <&clk 78>,
      <&clk 79>,
      <&clk 184>,
      <&clk 185>,
      <&clk 184>,
      <&clk 185>,
      <&clk 186>,
      <&clk 125>,
      <&clk 126>,
      <&clk 130>,
      <&clk 187>;
    clock-names = "disp_axi", "disp_apb",
           "disp_axi_root", "disp_apb_root",
           "lcdif-axi", "lcdif-apb", "lcdif-pix",
           "dsi-pclk", "dsi-ref",
