   assigned-clocks = <&clk 212>,
       <&clk 213>,
       <&clk 81>,
       <&clk 82>,
       <&clk 22>;
   assigned-clock-parents = <&clk 42>,
        <&clk 42>,
        <&clk 56>,
        <&clk 56>;
   assigned-clock-rates = <400000000>,
            <400000000>,
            <800000000>,
            <400000000>,
            <1200000000>;
   power-domains = <&pgc_gpumix>;
  };

  gic: interrupt-controller@38800000 {
   compatible = "arm,gic-v3";
   reg = <0x38800000 0x10000>,
         <0x38880000 0xc0000>;
   #interrupt-cells = <3>;
