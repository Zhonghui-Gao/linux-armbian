   interrupts = <0 25 4>;
   clocks = <&clk 247>,
     <&clk 263>,
     <&clk 102>;
   clock-names = "core", "bus", "reg";
   assigned-clocks = <&clk 309>;
   assigned-clock-parents = <&clk 56>;
   assigned-clock-rates = <800000000>;
